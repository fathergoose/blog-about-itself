{
  "$schema": "https://json.schemastore.org/tsconfig",
  "display": "Node 20",
  "_version": "20.1.0",
  "files": ["bin/build-pages.ts"],

  "compilerOptions": {
    "lib": ["ESNext"],
    "module": "node16",
    "target": "es2022",

    "esModuleInterop": true,
    "skipLibCheck": true,
    "forceConsistentCasingInFileNames": true,
    "moduleResolution": "node16"
  }
}
